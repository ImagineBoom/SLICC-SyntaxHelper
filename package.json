{
  "name": "gem5-slicc",
  "displayName": "gem5-slicc",
  "description": "Syntax highlighting for gem5-slicc",
  "version": "1.4.1",
  "publisher": "Tsinghua-Hexin-Joint-Institute",
  "icon": "images/icons/icon_512x512@2x.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "gem5-slicc",
        "icon": {
          "light": "images/file_icons/icon_512x512@2x.png",
          "dark": "images/file_icons/icon_512x512@2x.png"
        },
        "aliases": [
          "gem5-slicc"
        ],
        "extensions": [
          ".sm",
          ".slicc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gem5-slicc",
        "scopeName": "source.slicc",
        "path": "./syntaxes/gem5-slicc.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jest": "^29.5.8",
    "@types/mocha": "^10.0.4",
    "@types/vscode": "^1.82.0",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "semantic-release": "^22.0.5"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImagineBoom/gem5-slicc-support.git"
  }
}
